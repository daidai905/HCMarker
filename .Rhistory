celltype = character(),
gene_repeats = numeric(),
stringsAsFactors = FALSE
)
t <- unique(jj$cell_type)
t
for (i in t){
sub <- jj[jj$cell_type == i,]
# 计算每个细胞类型中基因的重复数
gene_counts <- table(sub$gene)
# 将结果转换为数据框，并添加细胞类型列
sub_result <- data.frame(gene = names(gene_counts),
celltype = i,
gene_repeats = as.numeric(gene_counts),
stringsAsFactors = FALSE)
